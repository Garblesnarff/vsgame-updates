{
  "name": "vampire-survival-game",
  "version": "1.0.0",
  "description": "A vampire-themed survival game",
  "main": "index.html",
  "scripts": {
    "start": "http-server -p 3000",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "typedoc --entryPointStrategy expand ./scripts --exclude '**/__tests__/**/*'",
    "lint": "eslint 'scripts/**/!(|-*).{js,ts}'",
    "lint:fix": "eslint 'scripts/**/!(|-*).{js,ts}' --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "game",
    "javascript",
    "typescript",
    "vampire",
    "survival"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "babel-jest": "^29.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}